{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "never"
        ]
    },
    "globals": {
        "describe": true,
        "it": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}